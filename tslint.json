{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "member-ordering": false,
    "curly": false
  },
  "linterOptions": {
    "exclude": [
      "*.json",
      "**/*.json",
      "**/*.d.ts",
      "**/node_modules/**/*",
      "dist/**/*",
      "build/**/*",
      "config/**/*.js",
      "coverage/lcov-report/*.js"
    ]
  }
}
